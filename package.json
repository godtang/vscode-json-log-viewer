{
	"name": "json-table-viewer",
	"displayName": "JSON Table Viewer",
	"description": "View JSON as Nested Table",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:json-table-viewer.helloWorld",
		"onCommand:json-table-viewer.OpenView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-table-viewer.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "json-table-viewer.OpenView",
				"title": "JSON Table Viewer: Open View"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.53.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
